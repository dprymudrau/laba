<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "org/apache/ibatis/builder/xml/mybatis-3-mapper.dtd">
<mapper namespace = "com.solvd.deliveryProject.dao.interfaces.ICarDAO">
    <select id="getById" parameterType ="int" resultMap="carResultMap">
        SELECT  c.carId as car_carId,
        c.carName as car_carName,
        c.numberOfTheCar as car_numberOfTheCar
        FROM Car c
        WHERE c.carId = #{id};
    </select>
    <resultMap id="CarsResultMap" type="com.solvd.laba.pojo.Car">
        <id column="car_carId" property="carId"/>
        <result column="car_carName" property="carName"/>
        <result column="car_numberOfTheCar" property="numberOfTheCar"/>
    </resultMap>
</mapper>