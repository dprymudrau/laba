<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "org/apache/ibatis/builder/xml/mybatis-3-mapper.dtd">
<mapper namespace = "com.solvd.deliveryProject.dao.interfaces.IOrdersDAO">
    <select id="getById" parameterType ="int" resultMap="ResultMap">
        SELECT  r.orderId as orders_orderId,
        c.orderName as orders_orderName
        FROM Orders r
        WHERE o.orderId = #{id};
    </select>
    <resultMap id="OrdersResultMap" type="com.solvd.laba.binary.Orders">
        <id column="orders_orderId" property="orderId"/>
        <result column="orders_orderName" property="orderName"/>
    </resultMap>
</mapper>